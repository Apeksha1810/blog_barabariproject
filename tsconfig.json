{
  "extends": "astro/tsconfigs/strict.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitAny": true,
    "paths": {
      "@": [
        "src/consts.ts"
      ],
      "@component:astro/*": [
        "src/components/*"
      ],
      "@component:react": [
        "components/react/index.ts"
      ],
      "@component:vue": [
        "components/vue/index.ts"
      ],
      "@component:preact": [
        "components/preact/index.ts"
      ],
      "@component:svelte": [
        "components/svelte/index.ts"
      ],
      "@component:vue/*": [
        "components/vue/index.ts"
      ],
    }
  },
}